<SOURCE>
    <ID>odm6_malarianone</ID>
    <ENDPOINT>http://localhost:3030/odm6_malarianone/sparql</ENDPOINT>
    <VAR>?x</VAR>
    <PAGESIZE>-1</PAGESIZE>
    <RESTRICTION>?x rdf:type ibge:Cidade</RESTRICTION>
    <PROPERTY>gn:name</PROPERTY>
</SOURCE>
<TARGET>
    <ID>linkedgeodata</ID>
    <ENDPOINT>http://linkedgeodata.org/sparql</ENDPOINT>
    <VAR>?y</VAR>
    <PAGESIZE>1000</PAGESIZE>
    <RESTRICTION>?y rdf:type lgdo:Place</RESTRICTION>
    <RESTRICTION>?y lgdo:is_in%3Acountry "Brazil"</RESTRICTION>
    <PROPERTY>rdfs:label AS nolang RENAME nome</PROPERTY>
</TARGET>
<METRIC>
   trigram(x.gn:name,y.nome)
</METRIC>
<ACCEPTANCE>
    <THRESHOLD>0.98</THRESHOLD>
    <FILE>accepted.N3</FILE>
    <RELATION>owl:sameAs</RELATION>
</ACCEPTANCE>
<REVIEW>
    <THRESHOLD>0.95</THRESHOLD>

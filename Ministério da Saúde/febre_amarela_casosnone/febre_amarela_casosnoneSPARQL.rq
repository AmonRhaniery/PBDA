PREFIX ds: <http://dados.gov.br/dataset/febre_amarela_casos#> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ibge: <https://metadados.ibge.gov.br/#> 
PREFIX gn: <http://www.geonames.org/ontology#> 
PREFIX j.0: <http://w3c/future-csv-vocab/> 
PREFIX j.1: <file:///Users/amonrhaniery/eclipse-workspace/hierarquiaurbana/input/febre_amarela_casosnone.csv#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX obo: <http://purl.obolibrary.org/obo/>
PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
CONSTRUCT {

    ?csv a dcat:Distribution;
        dcat:downloadURL ?downuri;
        dcat:mediaType "text/csv" .

    ?dataset a dcat:Dataset;
        dcat:landingPage ?uri;
        dcat:distribution ?csv ;
        rdfs:label "Municípios com registro de casos de Febre Amarela entre 2001 e 2013"@pt;
        rdfs:label "Cities with cases of yellow fever between 2001 and 2013"@en;
        rdfs:comment "O mapa mostra o número de casos de febre amarela que ocorrem nos municípios da Federação no período de 2001 a 2013Nível de Agregação: Município Periodicidade de Atualização: Anual Método de Cálculo: Total de casos de febre amarela no período"@pt .

    ?casosDoenca a rdf:Property;
        rdfs:domain ibge:Cidade ;
        rdfs:label "Teve registro de casos desta doença entre 2001 e 2013"@pt;
        rdfs:label "Had at least one case of this disease between 2001 and 2013."@en;
        rdfs:range obo:VO_0000001.

     ?city a ibge:Cidade ;
        gn:name ?no_cidade ;
        ds:casosDoenca obo:DOID_9682 .
        
} WHERE {
    ?row j.1:no_cidade ?no_cidade .
    BIND(ENCODE_FOR_URI( STR( ?no_cidade ) ) AS ?nome2)
    BIND(URI(CONCAT("https://cidades.ibge.gov.br/#", STR( ?nome2 )))  AS ?city)
    BIND(URI("http://dados.gov.br/dataset/febre_amarela_casos#csv")AS ?csv)
    BIND(URI("ttp://dados.gov.br/dataset/febre_amarela_casos")AS ?uri)
    BIND(URI("http://i3geo.saude.gov.br/i3geo/ogc.php?service=WFS&version=1.0.0&request=GetFeature&typeName=febre_amarela_casos&outputFormat=CSV")AS ?downuri)
    BIND(URI("http://dados.gov.br/dataset/febre_amarela_casos")AS ?dataset)
    BIND(URI(CONCAT("http://dados.gov.br/dataset/febre_amarela_casos#", "casosDoenca"))  AS ?casosDoenca)
}
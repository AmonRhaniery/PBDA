<SOURCE>
    <ID>acs_equipesnone</ID>
    <ENDPOINT>http://localhost:3030/acs_equipesnone/sparql</ENDPOINT>
    <VAR>?x</VAR>
    <PAGESIZE>-1</PAGESIZE>
    <RESTRICTION>?x rdf:type org:OrganizationUnit</RESTRICTION>
    <PROPERTY>geo:lat</PROPERTY>
    <PROPERTY>geo:long</PROPERTY>
    <PROPERTY>org:hasSite/gn:name RENAME nomecidade</PROPERTY>
    <FUNCTION>toWktPoint(geo:long, geo:lat) RENAME geometriaum</FUNCTION>
</SOURCE>
<TARGET>
    <ID>geonames</ID>
    <ENDPOINT>http://lod.openlinksw.com/sparql</ENDPOINT>
    <VAR>?y</VAR>
    <PAGESIZE>1000</PAGESIZE>
    <RESTRICTION>?y rdf:type gn:Feature</RESTRICTION>
    <RESTRICTION>?y gn:countryCode "BR"</RESTRICTION>
    <PROPERTY>geo:geometry RENAME geometriadois</PROPERTY>
    <PROPERTY>rdfs:label AS nolang RENAME nome</PROPERTY>
</TARGET>
<METRIC>
    AND(trigram(x.nomecidade,y.nome)|0.98,geo_min(x.geometriaum,y.geometriadois)|0.01)
</METRIC>
<ACCEPTANCE>
    <THRESHOLD>0.03</THRESHOLD>
    <FILE>accepted.N3</FILE>
    <RELATION>org:hasSite</RELATION>
</ACCEPTANCE>
<REVIEW>
    <THRESHOLD>0.01</THRESHOLD>
    <FILE>reviewme.N3</FILE>
    <RELATION>org:hasSite</RELATION>
</REVIEW>
<OUTPUT>N3</OUTPUT>
</LIMES>

<SOURCE>
    <ID>vwpess65oumaiss</ID>
    <ENDPOINT>http://localhost:3030/vwpess65oumais/sparql</ENDPOINT>
    <VAR>?x</VAR>
    <PAGESIZE>-1</PAGESIZE>
    <RESTRICTION>?x rdf:type ibge:Cidade</RESTRICTION>
    <PROPERTY>gn:name</PROPERTY>
    <PROPERTY>gn:parentADM1 AS replace(ACRE,Acre)->replace(ALAGOAS,Alagoas)->replace(AMAPÁ,Amapá)->replace(AMAZONAS,Amazonas)->replace(BAHIA,Bahia)->replace(CEARÁ,Ceará)->replace(DISTRITO FEDERAL,Distrito Federal)->replace(ESPIRITO SANTO,Espírito Santo)->replace(GOIÁS,Goiás)->replace(MARANHÃO,Maranhão)->replace(MATO GROSSO,Mato Grosso)->replace(MATO GROSSO DO SUL,Mato Grosso do Sul)->replace(MINAS GERAIS,Minas Gerais)->replace(PARÁ,Pará)->replace(PARAÍBA,Paraíba)->replace(PARANÁ,Paraná)->replace(PERNAMBUCO,Pernambuco)->replace(PIAUÍ,Piauí)->replace(RIO DE JANEIRO,Rio de Janeiro)->replace(RIO GRANDE DO NORTE,Rio Grande do Norte)->replace(RIO GRANDE DO SUL,Rio Grande do Sul)->replace(RONDÔNIA,Rondônia)->replace(RORAIMA,Roraima)->replace(SANTA CATARINA,Santa Catarina)->replace(SÃO PAULO,São Paulo)->replace(SERGIPE,Sergipe)->replace(TOCANTINS,Tocantins) RENAME estado</PROPERTY>
</SOURCE>
<TARGET>
    <ID>geonames</ID>
    <ENDPOINT>http://lod.openlinksw.com/sparql</ENDPOINT>
    <VAR>?y</VAR>
    <PAGESIZE>1000</PAGESIZE>
    <RESTRICTION>?y rdf:type spatial:Feature</RESTRICTION>
    <RESTRICTION>?y gadm:in_country "Brazil"</RESTRICTION>
    <PROPERTY>gadm:in_region</PROPERTY>
    <PROPERTY>rdfs:label AS nolang RENAME nome</PROPERTY>
</TARGET>
<METRIC>
    AND(trigram(x.gn:name,y.nome)|0.95,trigram(x.estado,y.gadm:in_region)|0.95)
</METRIC>
<ACCEPTANCE>
    <THRESHOLD>0.98</THRESHOLD>
    <FILE>accepted.N3</FILE>
    <RELATION>owl:sameAs</RELATION>
</ACCEPTANCE>
<REVIEW>
    <THRESHOLD>0.95</THRESHOLD>
    <FILE>reviewme.N3</FILE>
    <RELATION>owl:sameAs</RELATION>
</REVIEW>
<OUTPUT>N3</OUTPUT>

<SOURCE>
    <ID>hierarquiaurbana</ID>
    <ENDPOINT>http://localhost:3030/hierarquiaurbana/sparql</ENDPOINT>
    <VAR>?x</VAR>
    <PAGESIZE>-1</PAGESIZE>
    <RESTRICTION>?x rdf:type ibge:Cidade</RESTRICTION>
    <PROPERTY>gn:name</PROPERTY>
    <PROPERTY>gn:parentADM1</PROPERTY>
    <PROPERTY>gn:parentADM1 AS replace(AC,Acre)->replace(AL,Alagoas)->replace(AP,Amapá)->replace(AM,Amazonas)->replace(BA,Bahia)->replace(CE,Ceará)->replace(DF,Distrito Federal)->replace(ES,Espírito Santo)->replace(GO,Goiás)->replace(MA,Maranhão)->replace(MT,Mato Grosso)->replace(MS,Mato Grosso do Sul)->replace(MG,Minas Gerais)->replace(PA,Pará)->replace(PB,Paraíba)->replace(PR,Paraná)->replace(PE,Pernambuco)->replace(PI,Piauí)->replace(RJ,Rio de Janeiro)->replace(RN,Rio Grande do Norte)->replace(RS,Rio Grande do Sul)->replace(RO,Rondônia)->replace(RR,Roraima)->replace(SC,Santa Catarina)->replace(SP,São Paulo)->replace(SE,Sergipe)->replace(TO,Tocantins) RENAME estado</PROPERTY>
</SOURCE>
<TARGET>
    <ID>geonames</ID>
    <ENDPOINT>http://lod.openlinksw.com/sparql</ENDPOINT>
    <VAR>?y</VAR>
    <PAGESIZE>1000</PAGESIZE>
    <RESTRICTION>?y rdf:type spatial:Feature</RESTRICTION>
    <RESTRICTION>?y gadm:in_country "Brazil"</RESTRICTION>
    <PROPERTY>gadm:in_region</PROPERTY>
    <PROPERTY>rdfs:label AS nolang RENAME nome</PROPERTY>
</TARGET>
<METRIC>
    AND(trigram(x.gn:name,y.nome)|0.95,trigram(x.estado,y.gadm:in_region)|0.95)
</METRIC>
<ACCEPTANCE>
    <THRESHOLD>0.98</THRESHOLD>
    <FILE>accepted.N3</FILE>
    <RELATION>owl:sameAs</RELATION>
</ACCEPTANCE>
<REVIEW>
    <THRESHOLD>0.95</THRESHOLD>
    <FILE>reviewme.N3</FILE>
    <RELATION>owl:sameAs</RELATION>
</REVIEW>
<OUTPUT>N3</OUTPUT>
</LIMES>

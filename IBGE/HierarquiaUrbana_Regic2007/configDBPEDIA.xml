<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
<PREFIX>
    <NAMESPACE>https://metadados.ibge.gov.br/#</NAMESPACE>
    <LABEL>ibge</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
    <LABEL>owl</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://www.geonames.org/ontology#</NAMESPACE>
    <LABEL>gn</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
    <LABEL>rdfs</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
    <LABEL>rdf</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://www.w3.org/2003/01/geo/wgs84_pos#</NAMESPACE>
    <LABEL>geo</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://www.opengis.net/rdf#</NAMESPACE>
    <LABEL>ogc</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://geovocab.org/spatial#</NAMESPACE>
    <LABEL>spatial</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://linkedgeodata.org/ontology/</NAMESPACE>
    <LABEL>lgdo</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
    <LABEL>dbo</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://dbpedia.org/resource/</NAMESPACE>
    <LABEL>dbr</LABEL>
</PREFIX>
<SOURCE>
    <ID>hierarquiaurbana</ID>
    <ENDPOINT>http://localhost:3030/hierarquiaurbana/sparql</ENDPOINT>
    <VAR>?x</VAR>
    <PAGESIZE>-1</PAGESIZE>
    <RESTRICTION>?x rdf:type ibge:Cidade</RESTRICTION>
    <PROPERTY>gn:name</PROPERTY>
    <PROPERTY>ogc:asWKT RENAME geometriaum</PROPERTY>
</SOURCE>
<TARGET>
    <ID>dbpedia</ID>
    <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
    <VAR>?y</VAR>
    <PAGESIZE>1000</PAGESIZE>
    <RESTRICTION>?y rdf:type dbo:Place</RESTRICTION>
    <RESTRICTION>?y dbo:country dbr:Brazil</RESTRICTION>
    <PROPERTY>rdfs:label AS nolang RENAME nome</PROPERTY>
    <PROPERTY>geo:geometry RENAME geometriadois</PROPERTY>
</TARGET>
<METRIC>
    AND(trigram(x.gn:name,y.nome)|0.98,geo_hausdorff(x.geometriaum,y.geometriadois)|0.01)
</METRIC>
<ACCEPTANCE>
    <THRESHOLD>0.03</THRESHOLD>
    <FILE>accepted.N3</FILE>
    <RELATION>owl:sameAs</RELATION>
</ACCEPTANCE>
<REVIEW>
    <THRESHOLD>0.01</THRESHOLD>
    <FILE>reviewme.N3</FILE>
    <RELATION>owl:sameAs</RELATION>
</REVIEW>
<OUTPUT>N3</OUTPUT>
</LIMES>
